{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "preserve",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/pages/account/[id]/index.js", "src/components/Navbar.jsx", "src/components/task/EditAddTask.jsx", "src/components/profile/profile.jsx", "src/components/activity/activity.jsx", "src/components/signup/signup.jsx", "src/components/refferal/refferal.jsx", "src/components/EditProfile/editProfile.jsx"  ],
  "exclude": [
    "node_modules"
  ],
  "jsx": "react",
}
